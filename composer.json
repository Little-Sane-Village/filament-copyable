{
    "name": "saadj55/filament-copyable",
    "description": "Filament fields and columns with copyable values",
    "autoload": {
        "psr-4": {
            "Saadj55\\FilamentCopyable\\": "src/"
        }
    },
    "version": "0.1.2",
    "authors": [
        {
            "name": "Saad Nasir",
            "email": "saadj55@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.9",
        "spatie/laravel-package-tools": "^1.10"
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saadj55\\FilamentCopyable\\FilamentCopyableProvider"
            ]
        }
    },
    "prefer-stable": true
}
